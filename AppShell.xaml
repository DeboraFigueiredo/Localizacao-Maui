<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Localizacao.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Localizacao"
    Shell.FlyoutBehavior="Disabled"
    Title="Localizacao">

    <ShellContent
        Title="Localização"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <Shell.TitleView>
        <Label Text="Localização" FontSize="30" FontAttributes="Bold" HorizontalOptions="Center"
               Margin="0,10,0,0"/>
    </Shell.TitleView>


</Shell>
